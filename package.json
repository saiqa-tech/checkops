{
  "name": "@saiqa-tech/checkops",
  "version": "3.0.0",
  "description": "A high-performance, production-ready Node.js package for creating dynamic forms with advanced optimizations, real-time monitoring, centralized question reusability, and enterprise-grade performance capabilities",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --runInBand",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules jest tests/unit --maxWorkers=4",
    "test:integration": "NODE_OPTIONS=--experimental-vm-modules jest tests/integration --runInBand",
    "test:performance": "NODE_OPTIONS=--experimental-vm-modules jest tests/performance",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage --runInBand",
    "test:fast": "NODE_OPTIONS=--experimental-vm-modules jest tests/unit --maxWorkers=4",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:ci": "NODE_OPTIONS=--experimental-vm-modules jest tests/unit --maxWorkers=4",
    "test:all": "NODE_OPTIONS=--experimental-vm-modules jest --coverage --verbose --runInBand",
    "test:power": "NODE_OPTIONS=--experimental-vm-modules jest tests/checkops-power",
    "test:baseline": "NODE_OPTIONS=--experimental-vm-modules jest --maxWorkers=1 --logHeapUsage tests/unit",
    "migrate": "node migrations/run.js",
    "lint": "eslint src tests",
    "format": "prettier --write \"src/**/*.js\" \"tests/**/*.js\""
  },
  "keywords": [
    "forms",
    "form-builder",
    "dynamic-forms",
    "questionnaire",
    "survey",
    "checkops",
    "submission",
    "postgresql",
    "jsonb",
    "performance",
    "monitoring",
    "optimization",
    "enterprise",
    "high-performance",
    "batch-operations",
    "caching",
    "metrics"
  ],
  "author": "Saiqa Tech",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/saiqa-tech/checkops.git"
  },
  "bugs": {
    "url": "https://github.com/saiqa-tech/checkops/issues"
  },
  "homepage": "https://github.com/saiqa-tech/checkops#readme",
  "peerDependencies": {
    "pg": "^8.11.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "express": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "supertest": "^7.1.4"
  },
  "files": [
    "src",
    "migrations",
    "docs",
    "README.md",
    "LICENSE"
  ]
}