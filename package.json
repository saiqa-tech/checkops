{
  "name": "@checkops/form-builder-submission-sdk",
  "version": "0.2.0",
  "description": "SDK for validating and submitting CheckOps form builder submissions with full backend support.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./server": {
      "import": "./dist/server/index.js",
      "require": "./dist/server/index.cjs",
      "types": "./dist/server/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/server/index.ts --format esm,cjs --dts --clean --sourcemap",
    "build:sdk": "tsup src/index.ts --format esm,cjs --dts --clean --sourcemap",
    "build:server": "tsup src/server/index.ts --format esm,cjs --dts --clean --sourcemap",
    "test": "vitest run tests/sdk.test.ts tests/backend.test.ts tests/build.test.ts",
    "test:watch": "vitest tests/sdk.test.ts tests/backend.test.ts tests/build.test.ts",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:all": "vitest run",
    "migrate": "node -r tsx/dist/register scripts/migrate.ts",
    "dev": "tsx watch src/server/index.ts",
    "start": "node dist/server/index.js",
    "clean": "rimraf dist"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "checkops",
    "form",
    "sdk",
    "submission",
    "backend",
    "database",
    "api"
  ],
  "author": "CheckOps",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "pg": "^8.11.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "@types/pg": "^8.10.9",
    "rimraf": "^5.0.5",
    "tslib": "^2.6.2",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6",
    "tsx": "^4.6.2",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16"
  }
}
